<Window x:Class="CSharpProject_ASkek.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharpProject_ASkek"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<ToolBar HorizontalAlignment="Left" Height="29" VerticalAlignment="Top" Width="794" Margin="0,0,-0.4,0" Header="Menu: ">
			<MenuItem x:Name="Menu_Open" Header="Open" Click="Menu_Open_Click"/>
			<MenuItem Header="Save"/>
			<MenuItem x:Name="Menu_Save" Cursor="Arrow" Header="Sort" Click="Menu_Save_Click">
				<Button Content="Item Code"/>
				<Button Content="Current Count"/>
				<Button Content="On Order"/>
			</MenuItem>
		</ToolBar>
	</Grid>
</Window>
